execute pathogen#infect()
syntax on
filetype indent plugin on
set background=dark
set textwidth=79
set shiftwidth=4
set tabstop=4
set expandtab
set softtabstop=4
set shiftround
set autoindent
set smarttab
set autochdir
set incsearch
set hlsearch
set showmatch
set ignorecase
set smartcase
colorscheme industry
set tags=./tags,tags;$HOME
"autocmd vimenter * NERDTree
